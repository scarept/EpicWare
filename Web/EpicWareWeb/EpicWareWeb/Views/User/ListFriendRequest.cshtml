@model IEnumerable<EpicWareWeb.Models.FriendRequest>

@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>ListFriendRequest</title>
</head>
<body>
    @if (Model.Count() > 0)
    {
        <h2>Pedidos de amizade pendentes</h2>
    <table>
        @foreach (var item in Model)
        {
            @Html.Partial("_FriendRequest", item)
        }
    </table>
    }
    else
    {
        <h2>Sem pedidos de amizade pendentes</h2>
    }
    
</body>
</html>
