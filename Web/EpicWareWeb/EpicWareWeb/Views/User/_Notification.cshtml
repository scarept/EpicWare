@model EpicWareWeb.Models.Notification
<tr style="color:white;">
    <td><span style="font-size:14px; color:white;">@Model.message</span></td>
    <td><span style="font-size:14px; color:white;">@Model.time.Hour:@Model.time.Minute</span></td>
    <td><form action="@Model.notificationType.resultLink"><button class="btn btn-info">@EpicWareWeb.Views.User.User.ver</button></form></td>
</tr>