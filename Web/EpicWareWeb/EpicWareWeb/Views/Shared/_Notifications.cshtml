@model EpicWareWeb.Models.User
@{
    int count = 0;
    foreach (EpicWareWeb.Models.Notification item in Model.notifications)
    {
        if (!item.read)
        {
            count++;
        }
    }
}
@using (Html.BeginForm("ListNotifications","User")){
    <input type="submit" value="@EpicWareWeb.Views.User.User.notificacoes : @count" />
}           
